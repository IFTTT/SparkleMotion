<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ifttt.jazzhandsdemo">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.ifttt.jazzhandsdemo.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".AlphaViewPagerActivity" />

        <activity android:name=".ScaleViewPagerActivity" />

        <activity android:name=".RotationViewPagerActivity" />

        <activity android:name=".ParallaxViewPagerActivity" />

        <activity android:name=".ZoomOutViewPagerActivity" />

        <activity android:name=".PathAndTranslationViewPagerActivity" />

    </application>

</manifest>
